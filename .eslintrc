{
    "env": {
        "node": true,
        "browser": false,
        "es6": true,
        "jest": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "no-unused-vars": "warn"
    },
    "ecmaFeatures": {
        "spread": true,
        "arrowFunctions": true,
        "experimentalObjectRestSpread": true
    },
    "parserOptions": {
        "ecmaVersion": 8
    }
}
